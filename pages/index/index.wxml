<view class="container">
    <view class="wrapper">
        <view class="random">{{selected}}</view>
        <view>
            <button bindtap="getSelected" class="btn">{{status}}</button>
        </view>
    </view>
    <navigator bindtap="randomEnd" class="bar" openType="navigate" url="/pages/setting/setting">自定义</navigator>
    <view class="fadeList">
        <view class="temp" style="{{item.style}}" wx:for="{{fadeList}}" wx:key="{{index}}">{{item.name}}</view>
    </view>
</view>
